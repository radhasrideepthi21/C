#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int number, digit, sum = 0;
    
    // Read the input (five-digit number)
    scanf("%d", &number);
    
    // Extract digits and sum them
    while (number > 0) {
        digit = number % 10;  // Get the last digit
        sum += digit;         // Add the digit to the sum
        number /= 10;         // Remove the last digit
    }
    
    // Print the result
    printf("%d\n", sum);
    
    return 0;
}
